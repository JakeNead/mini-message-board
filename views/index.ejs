<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <% messages.map((message) => { %>
    <div style="border: 1px solid black">
      <p><%= message.text %></p>
      <p>- <%= message.user %></p>
      <!-- <p><%= message.added %></p> -->
      <a href="/messages/<%= message.id %>"><button>Open</button></a>
    </div>
    
    <% }) %>
      <a href=/new > <button type="button">New Message</button></a
  >
  </body>
</html>
